<term_definitions code="at0000">
    <items id="text">nes_vaccination_protocol</items>
    <items id="description">NDS_Immunisation_Protocol. a small cluster to support recording additional details about immunization events.</items>
</term_definitions>
<term_definitions code="at0001">
    <items id="text">Target disease</items>
    <items id="description">Coded or textual data for the diseases against which the vaccination is intended to confer immunity.</items>
</term_definitions>
<term_definitions code="at0003">
    <items id="text">Series name</items>
    <items id="description">Local name or description for the immunisation e.g 'Influenza' 'Hepatitis A/B' 'Hepatitis A and Typhoid'</items>
</term_definitions>
<term_definitions code="at0004">
    <items id="text">Subject agreement to immunisation</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0005">
    <items id="text">Appointment ID</items>
    <items id="description">The ID of the 'appointment' from a scheduling system which was associated with this vaccination event.</items>
</term_definitions>
<term_definitions code="at0006">
    <items id="text">Series doses</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0007">
    <items id="text">Series doses description</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0008">
    <items id="text">Trial ID</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0009">
    <items id="text">Vaccination centre</items>
    <items id="description">A text element for the name or other description of the vaccination centre where this vaccination record originated.</items>
</term_definitions>
<term_definitions code="at0010">
    <items id="text">Vaccination centre details</items>
    <items id="description">A cluster slot to allow more details of the vaccination centre.</items>
</term_definitions>
<term_definitions code="at0011">
    <items id="text">Country of vaccination</items>
    <items id="description">To record the country of vaccination, ideally using a terminology value set.</items>
</term_definitions>
